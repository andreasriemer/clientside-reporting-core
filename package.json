{
  "name": "clientside-reporting-core",
  "version": "1.0.0",
  "description": "A clientside implementation for reports",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc && webpack",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "link": "npm link",
    "uglify": "uglifyjs -c -o lib/reporting-core.min.js  -- lib/**/*.js"
  },
  "files": [
    "lib /**/*"
  ],
  "keywords": [
    "clientside",
    "reporting",
    "report tool"
  ],
  "author": "Andreas Riemer",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/workerpool": "^6.1.0",
    "copy-webpack-plugin": "^9.0.1",
    "jest": "^27.0.6",
    "npm-watch": "^0.11.0",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.3",
    "webpack": "^5.54.0",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "workerpool": "^6.1.5"
  }
}
